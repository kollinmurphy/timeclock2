    "android": {
      "package": "com.canlin.clockwork",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "extra": {
      "eas": {
        "projectId": "5e1f05bd-e582-4a08-993e-e674c312cfe0"
      }
    }
  }
