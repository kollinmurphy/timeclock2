{
  "expo": {
    "name": "ClockWork",
    "slug": "timeclock2",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.kollinmurphy.clockwork"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.kollinmurphy.clockwork",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 7
    },
    "extra": {
      "eas": {
        "projectId": "5e1f05bd-e582-4a08-993e-e674c312cfe0"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-7143283411955031~6480244914",
    "ios_app_id": "ca-app-pub-7143283411955031~4563709772"
  }
}
